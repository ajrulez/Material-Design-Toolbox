        <attr name="bottomNavigationActiveTint" format="color"/>
        <attr name="bottomNavigationInactiveTint" format="color"/>
        <attr name="bottomNavigationDarkTheme" format="boolean"/>
        <attr name="bottomNavigationOrientation" format="enum">
